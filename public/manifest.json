{
  "name": "Saira data generator",
  "description": "Generate data for your saira",
  "version": "1.1",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "contextMenus",
    "storage"
  ]
}